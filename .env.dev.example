# .env.dev.example - Development environment template
# Copy to .env and fill in your values

# Backend URL (local dev)
VITE_API_URL=http://localhost:3001

# Demo mode (shows "DEMO" badge instead of "REAL ON-CHAIN")
VITE_ENABLE_DEMO_MODE=true

# WebSocket for new mints (optional - uses demo generator if not set)
# VITE_MINTS_STREAM_URL=wss://your-quicknode.solana-mainnet.quiknode.pro/token/

# Birdeye API (for price data)
VITE_BIRDEYE_API_KEY=your-birdeye-api-key

# Backend (copy to backend-node/.env)
# PORT=3001
# QUICKNODE_HTTP=https://your-endpoint.solana-mainnet.quiknode.pro/token/
# QUICKNODE_WSS=wss://your-endpoint.solana-mainnet.quiknode.pro/token/
# ADMIN_TOKEN=dev-admin-token-change-in-prod
# TRADING_PUBLIC=1
# FEE_WALLET=your-fee-wallet-address
# FEE_PERCENTAGE=1.0
